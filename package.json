{
    "name": "@hlustikp/rrt",
    "version": "0.1.0",
    "description": "Rest Test framework written in Rust",
    "main": "rrt.js",
    "bin": "rrt.js",
    "scripts": {
        "build": "cargo-cp-artifact -nc rrt.node -- cargo build -r --message-format=json-render-diagnostics",
